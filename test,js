const input = '{ "data": { "device_id": "40:22:D8:EA:06:F8", "humidity": 63, "temperature": 28}}';
const data = JSON.parse(input);
//extract device_id from data
const device_id = data.data.device_id;
//extract temperature from data
const temperature = data.data.temperature;

//extract humidity from data
const humidity = data.data.humidity;



// get current time of Ho Chi Minh City timezone
function getCurrentDateTime() {
    //get current time in UTC with format: "2023-10-06 06:46:06"
    const currentDateTime = new Date().toISOString();
    //convert current time to Ho Chi Minh City timezone
    const date = new Date(currentDateTime);
    date.setHours(date.getHours() + 7);
    //convert current time to format: "2023-10-06 13:46:06"
    const currentDateTimeInHCMC = date.toISOString();
    //get current date
    const currentDate = currentDateTimeInHCMC.split('T')[0];
    //get current time
    const currentTime = currentDateTimeInHCMC.split('T')[1].split('.')[0];
    return { currentDate, currentTime };
}